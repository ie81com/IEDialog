  "author": "ie81com",
  "homepage": "https://github.com/ie81com/iedialog",
  "repository": {
    "type": "git",
    "url": "https://github.com/ie81com/iedialog.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ie81com/iedialog/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/iedialog.umd.cjs",
  "module": "./dist/iedialog.js",
  "exports": {
    ".": {
